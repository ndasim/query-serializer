{
  "name": "query-serializer",
  "version": "1.0.3",
  "description": "QuerySerializer is a TypeScript class that provides serialization and deserialization of objects to and from URL-encoded query strings. It supports handling nested objects and arrays within the serialization and deserialization processes.",
  "main": "index.js",
  "types": "index.d.ts",
  "directories": {
    "lib": "src"
  },
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ndasim/query-serializer.git"
  },
  "bugs": "https://github.com/ndasim/query-serializer/issues",
  "homepage": "https://github.com/ndasim/query-serializer",
  "keywords": ["querystring", "query-string", "nested querystring","nested serializer", "serializer", "querystring deserializer"],
  "author": {
    "name" : "Asım Doğan NAMLI",
    "email" : "asim.dogan.namli@gmail.com",
    "url" : "https://github.com/ndasim"
  },
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/jest": "^29.5.2",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.1.3"
  },
  "files": ["/lib", "index.js", "index.d.js"]
}
